export MAIN_DIALOG='
<window title="Installed packages">
<vbox>
	<edit>
		<input file>/root/PACKAGES</input>
		<width>550</width>
		<height>350</height>
	</edit>
</vbox>
</window>
'

gtkdialog -c -p MAIN_DIALOG
